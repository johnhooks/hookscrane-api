{
  "test": ".*.ts$",
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "jsc": {
    "target": "es2020",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "paths": {
      "lib/*": ["lib/*"],
      "plugins/*": ["plugins/*"],
      "schema/*": ["schema/*"],
      "server/*": ["server/*"]
    }
  }
}
